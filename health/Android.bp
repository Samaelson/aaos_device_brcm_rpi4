cc_binary {
    name: "android.hardware.health-service.rpi",
    relative_install_path: "hw",
    vendor: true,
    init_rc: ["android.hardware.health-service.rpi.rc"],
    vintf_fragments: ["android.hardware.health-service.rpi.xml"],
